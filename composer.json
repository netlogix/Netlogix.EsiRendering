{
    "name": "netlogix/esi-rendering",
    "description": "Render content in fusion as esi:include",
    "license": "MIT",
    "type": "neos-package",
    "keywords": [
        "neos",
        "esi",
        "varnish",
        "nginx"
    ],
    "require": {
        "neos/neos": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "Netlogix\\EsiRendering\\": "Classes/"
        }
    },
    "suggest": {
        "flowpack/varnish": "For Varnish support including caching ESIs individually with their own lifetime"
    },
    "extra": {
        "neos": {
            "package-key": "Netlogix.EsiRendering"
        }
    }
}
